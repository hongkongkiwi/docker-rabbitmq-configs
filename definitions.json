{
  "rabbit_version": "3.5.7",
  "users": [{
    "name": "me",
    "password": "me",
    "tags": "administrator"
  }],
  "vhosts": [{
    "name": "/my_vhost"
  }],
  "permissions": [{
    "user": "me",
    "vhost": "/my_vhost",
    "configure": ".*",
    "write": ".*",
    "read": ".*"
  }],
  "parameters": [],
  "policies": [],
  "exchanges": [{
    "name": "my_exchange",
    "vhost": "/my_vhost",
    "type": "direct",
    "durable": true,
    "auto_delete": false,
    "internal": false,
    "arguments": {}
  }],
  "queues": [{
    "name": "my_queue",
    "vhost": "/my_vhost",
    "durable": false,
    "auto_delete": false,
    "arguments": {}
  }],
  "bindings": [ {
      "source": "my_exchange",
      "vhost": "/my_vhost",
      "destination": "my_queue",
      "destination_type": "queue",
      "routing_key": "test",
      "arguments": {}
    }]
}
